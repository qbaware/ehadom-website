---
import favIconSvg from '~/assets/favicons/ehadom_black.png';
import appleTouchIcon from '~/assets/favicons/ehadom_black.png';

const favIcon = '~/assets/favicons/ehadom_black.png';
---

<link rel="shortcut icon" href={favIcon} />
<link rel="icon" type="image/svg+xml" href={favIconSvg.src} />
